{
    "name": "sovic/user-bundle",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "UserBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "UserBundle\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Vojtech Bojda"
        }
    ],
    "repositories": [
        {
            "type": "git",
            "url": "https://github.com/sovic/common.git"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "~8.2",
        "doctrine/orm": "^3.5",
        "doctrine/dbal": "^3.8",
        "sovic/common": "dev-master",
        "symfony/cache": "^7.3",
        "symfony/form": "^7.3",
        "symfony/framework-bundle": "^7.3",
        "symfony/translation": "^7.3",
        "symfony/security-bundle": "^7.3",
        "symfony/mailer": "^7.3",
        "symfony/twig-bundle": "^7.3",
        "symfony/twig-bridge": "^7.3",
        "symfony/validator": "^7.3"
    }
}
